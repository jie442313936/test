
.hello {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
}
.title {
	color: #8f8f94;
	margin-top: 20rpx;
}
.ul {
	font-size: 30rpx;
	color: #8f8f94;
	margin-top: 50rpx;
}
.ul > view {
	line-height: 50rpx;
}
map {
	width: 100%;
	height: 600rpx;
}
.avator_text{
	margin-top: 15%;
	margin-left: 15%;
	font-size: 38rpx;
	font-weight: 800;
}
.avator_img{
	margin-top: 37%;
	margin-left: 41%;
}
.avator_img ._img{
	width: 128rpx;
	height: 128rpx;
}
.content ._p, .content ._span{
	width: 13px;
	height: 13px;
	border-radius:40%;
	-webkit-animation: myfirst 1.5s  infinite;
	        animation: myfirst 1.5s  infinite;
	-webkit-box-shadow: 0px 0px 1px #009FD9;
	        box-shadow: 0px 0px 1px #009FD9; 
	margin: 0px;
}
.content  ._img{
	-webkit-animation-delay: 0.8s;
	        animation-delay: 0.8s;
}
.content .one{position: absolute;left:7%; top:20.5%;}
.content .ten{background: #A2A9B4;position: absolute;left:300px; top:350px;}
@-webkit-keyframes myfirst{
10% {-webkit-transform: scale(1);transform: scale(1);}
100% {-webkit-transform: scale(8);transform: scale(8);}
}
@keyframes myfirst{
10% {-webkit-transform: scale(1);transform: scale(1);}
100% {-webkit-transform: scale(8);transform: scale(8);}
}

